{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://feedback-server-czv8.onrender.com/api",
    "VITE_SOCKET_URL": "https://feedback-server-czv8.onrender.com",
    "VITE_APP_PORT": "443",
    "VITE_APP_HOST": "0.0.0.0"
  }
}